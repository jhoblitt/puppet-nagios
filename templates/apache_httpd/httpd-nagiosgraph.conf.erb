# enable nagiosgraph CGI scripts
ScriptAlias /nagiosgraph/cgi-bin "/usr/libexec/nagiosgraph/cgi-bin"
<Directory "/usr/libexec/nagiosgraph/cgi-bin">
   Options ExecCGI
   AllowOverride None
   Order allow,deny
   Allow from all
</Directory>
# enable nagiosgraph CSS and JavaScript
Alias /nagiosgraph "/usr/share/nagiosgraph/htdocs"
<Directory "/usr/share/nagiosgraph/htdocs">
   Options None
   AllowOverride None
   Order allow,deny
   Allow from all
</Directory>

